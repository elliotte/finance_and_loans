
<article class="type-system-slab modal-form">

      <h1>default CSV import</h1>
      <p> ( check <a href="" target="_blank"> Template </a> column headers ) </p>
      <%= form_tag user_csv_import_reports_path, multipart: true do %>
               <%= hidden_field_tag :id , @report.id %>
                <table id="upload-bank-csv-form" class="table-minimal">
                  <tbody>
                      <tr>
                        <td class="text-pink"><%= file_field_tag :file %></td>
                        <td><%= submit_tag "Import CSV" %></td>
                      </tr>
                    </tbody>
                </table>
      <% end %> 

</article>